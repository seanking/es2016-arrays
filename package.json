{
  "name": "es2016 Features",
  "version": "1.0.0",
  "description": "ES2016 Features",
  "scripts": {
    "lint": "eslint .",
    "test": "mocha 'test/**/*.js'",
    "pretest": "npm run lint"
  },
  "eslintConfig": {
    "ecmaVersion": 7,
    "extends": "airbnb/base",
    "env": {
      "mocha": true
    },
    "rules": {
      "no-var": 0,
      "vars-on-top": 0,
      "block-scoped-var": 0,
      "no-console": 0,
      "no-loop-func": 0,
      "no-use-before-define": 0,
      "no-sparse-arrays": 0
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seanking/es2016-features.git"
  },
  "author": "Sean King",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/seanking/es2016-features/issues"
  },
  "homepage": "https://github.com/seanking/es2016-featuress#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "^3.2.0"
  }
}
